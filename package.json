  "scripts": {
    "build": "webpack --mode=production",
    "dev:build": "webpack --mode=development --watch",
    "dev:start": "nodemon --trace-sync-io server/start.js",
    "start": "node server/start.js",
    "db:setup": "psql property_service -f database_sql/seedScripts/setup.sql",
    "db:makeMocks": "bash database_sql/seedScripts/make_1M_mocks.sh",
    "db:loadMocks": "psql property_service -f database_sql/seedScripts/copy1M.sql",
    "db:makeFeatures": "node database_sql/featureSeed.js",
    "db:loadFeatures": "psql property_service -f database_sql/seedScripts/features.sql",
    "db:mockRange": "node database_sql/mockMaker",
    "test": "node_modules/jest/bin/jest.js spec --runInBand",
    "clean": "rm -rf client/dist",
    "clean:build": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-prettify": "^0.1.1",
    "newrelic": "^6.11.0",
    "pg": "^8.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "csv-write-stream": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "jest-cli": "^25.3.0",
    "jest-enzyme": "^7.1.2",
    "lorem-ipsum": "^2.0.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.3",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
